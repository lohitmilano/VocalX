{
  "name": "vocalx-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "lint": "turbo lint",
    "typecheck": "turbo typecheck",
    "affiliate:dev": "turbo run dev --filter=affiliate-webapp",
    "affiliate:build": "turbo run build --filter=affiliate-webapp"
  },
  "devDependencies": {
    "turbo": "^2.5.6"
  }
}
